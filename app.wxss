/**app.wxss**/
/* @import "weui.wxss"; */
@import "/common/wxss/list-view";
@import "/common/tabbar/tab.wxss";
page {
  height: 100%;
}
.container {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
  position: relative;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.clearfix {
  clear: both;
  display: block;
  visibility: hidden;
  height: 0;
}
.clearfix::after {
  content: ' ';
  display: inline-block;
}
