.tabbar_box{
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 999;
  width: 100%;
  height: 88px;
  border-top: 1rpx solid #fff; 
}

.tabbar_nav{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 16px;
  height: 100%;
}

.tabbar_icon{
  width: 48px;
  height: 48px;
}